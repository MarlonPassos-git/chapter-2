{
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.formatOnSave": true,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/dist": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"**/commitlint.config.js": true,
		"**/sonar-project.properties": true,
		"**/yarn.lock": true,
		"**/lerna.json": true,
		"**/Dockerfile": true,
		"**/bitbucket-pipelines.yml": true,
		"**/.dockerignore": true,
		"**/.editorconfig": true,
		"**/.gitattributes": true,
		"**/.gitignore": true,
		"**/.gitpod.yml": true,
		"**/.husky": true,
		"**/node_modules": true,
		"**/.yarn": true,
		"**/.eslintignore": true,
		"**/.eslintrc.js": true,
		"**/.vtexignore": true,
		"**/gulpfile.js": true,
		"**/CHANGELOG.md": true,
		"**/yarn-error.log": true,
		"**/react/**/*.css": true,
		"**/package-lock.json": true,
		"**/tsconfig.json": true,
		"**/.eslintrc.json": false,
		"**/.prettierrc": false
	}
}
